{
    "model": "mobilenet_v2",
    "pretrained": true,
    "input_info": {
      "sample_size": [2, 3, 32, 32]
    },
    "num_classes": 100,
    "batch_size" : 64,
    "checkpoint_save_dir": "results/snapshots",
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.001,
        "schedule_type": "multistep",
        "steps": [20, 30, 40]
    },
    "compression": [
        {
            "algorithm": "rb_sparsity",
            "sparsity_init": 0.01,
            "params": {
                "sparsity_target": 0.52,
                "sparsity_target_epoch": 5,
                "sparsity_freeze_epoch": 10
            },
            "ignored_scopes": ["MobileNetV2/Sequential[features]/ConvBNReLU[0]/NNCFConv2d[0]"]
        },
        {
            "algorithm": "quantization",
            "initializer": {
                "range": {
                    "num_init_samples": 256,
                    "type": "min_max"
                }
            }
        }
    ],
    "epochs": 40,
    "dataset": "CIFAR100"
}
