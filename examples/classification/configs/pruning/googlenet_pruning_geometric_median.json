{
    "model": "googlenet",
    "batch_size" : 150,
    "pretrained": true,
    "epochs": 110,
    "input_info": {
        "sample_size": [
            1,
            3,
            224,
            224
        ]
    },
    "optimizer": {
        "type": "SGD",
        "base_lr": 0.1,
        "weight_decay": 1e-4,
        "schedule_type": "multistep",
        "steps": [
            30,
            50,
            70,
            90
        ],
        "optimizer_params":
        {
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "compression": [
       {
            "algorithm": "filter_pruning",
            "pruning_init": 0.1,
            "params": {
                "schedule": "exponential",
                "pruning_target": 0.5,
                "pruning_steps": 25,
                "weight_importance": "geometric_median"
            }
       }
    ]
}
