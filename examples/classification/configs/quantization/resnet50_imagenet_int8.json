{
    "model": "resnet50",
    "pretrained": true,

    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size": 85,
    "epochs": 6,
    "multiprocessing_distributed": true,
    "optimizer": {
        "type": "Adam",
        "base_lr": 1e-5,
        "schedule_type": "multistep",
        "steps": [
            3
        ]
    },
    "compression": {
        "algorithm": "quantization",
        "params": {
            "waveq": true,
            "ratio": 6e-04
        },
        "initializer": {
            "range": {
                "num_init_steps": 10
            }
        }
    }
}
