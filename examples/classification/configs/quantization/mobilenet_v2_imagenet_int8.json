{
    "model": "mobilenet_v2",
    "pretrained": true,
    "input_info": {
      "sample_size": [2, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size" : 256,
    "epochs": 7,
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.00001,
        "schedule_type": "multistep",
        "steps": [
            5
        ],
    },
    "compression": {
        "algorithm": "quantization",
        "params": {
            "waveq": true,
            "ratio": 15e-01
        },
        "activations": {
            "mode": "asymmetric",
            "bits": 8
        },
        "weights": {
            "mode": "asymmetric",
            "bits": 8
        },
        "initializer": {
            "range": {
                "num_init_steps": 10
            }
        }
    }
}