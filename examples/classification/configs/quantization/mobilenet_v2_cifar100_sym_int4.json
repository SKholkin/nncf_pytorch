{
    "model": "mobilenet_v2",
    "input_info": {
      "sample_size": [2, 3, 32, 32]
    },
    "num_classes": 100,
    "batch_size": 256,
    "optimizer": {
        "type": "Adam",
        "base_lr": 1e-4,
        "schedule_type": "multistep",
        "steps": [
            5
        ]
    },
    "compression": {
        "algorithm": "quantization",
        "waveq":  true,
        "activations": {
            "mode": "symmetric",
            "signed": false,
            "bits": 4
        },
        "weights": {
            "mode": "symmetric",
            "signed": false,
            "bits": 4
        }
    },
    "epochs": 5,
    "dataset": "CIFAR100"
}