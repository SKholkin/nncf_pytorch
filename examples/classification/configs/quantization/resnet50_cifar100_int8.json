{
    "model": "resnet50",
    "input_info": {
      "sample_size": [2, 3, 32, 32]
    },
    "num_classes": 100,
    "batch_size": 256,
    "optimizer": {
        "type": "Adam",
        "base_lr": 1e-4,
        "schedule_type": "multistep",
        "steps": [
            5
        ]
    },
    "compression": {
        "algorithm": "quantization",
        "params": {
            "waveq": true,
            "ratio": 300,
            "schedule": true,
            "schedule_epoch_steps": [5, 10]
        },
        "activations": {
            "mode": "asymmetric",
            "bits": 3
        },
        "weights": {
            "mode": "asymmetric",
            "bits": 3
        }
    },
    "epochs": 5,
    "dataset": "CIFAR100"
}