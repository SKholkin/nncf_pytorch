{
    "model": "resnet50",
    "pretrained": true,

    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size": 85,
    "epochs": 1,
    "multiprocessing_distributed": true,
    "optimizer": {
        "base_lr": 0.00031,
        "schedule_type": "plateau",
        "type": "Adam",
        "scheduler_params": {
            "threshold": 0.1,
            "cooldown": 3
        },
        "weight_decay": 1e-05
    },
    "properties": {
        "algorithm": "quantization",
        "weights": {
            "mode": "asymmetric",
            "bits": 4
        },
        "per_channel": true,
        "initializer": {
            "num_init_steps": 50
        },
        "activations": {
            "mode": "asymmetric",
            "bits": 4
        },
        "quantize_inputs": true
    }
}

